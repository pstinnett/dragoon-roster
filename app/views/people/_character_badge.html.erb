<div class="media">
	<div class="media-left">
        <a href="#">
    	    <img src='http://www.bungie.net/<%= character.api_info['data']['emblemPath'] %>'/>
        </a>
	</div>
    <div class="media-body">
        <div class="row">
            <div class="col-md-1">
                <h4 class="media-heading"><%= character.classname %>
            </div>
            <div class="col-md-3 text-right">
                <p><strong><%= character.api_info['data']['characterBase']['powerLevel'] %></strong></p>
                <p><span class="glyphicon glyphicon-certificate" aria-hidden="true"></span> <%= character.api_info['data']['characterLevel'] %></p>
            </div>
        </div>
    </div>
</div>
